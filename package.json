  "repository": {
    "type": "git",
    "url": "git+https://github.com/TermFTP/TermFTP-Client"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "build:electron": "tsc --build tsconfig.electron.json",
    "test": "node scripts/test.js",
    "electron": "electron .",
    "preelectron-pack": "npm run build && npm run build:electron",
    "electron-pack": "electron-builder -c.extraMetadata.main=build/main.js --publish=never",
    "dev": "cross-env BROWSER=none NODE_ENV=development npm run devScript",
    "devScript": "concurrently \"npm run start\" \"wait-on http://localhost:3000 && npm run build:electron && npm run electron\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "build": {
    "publish": null,
    "appId": "at.termftp.client",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "util/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testRunner": "C:\\coding\\coop\\TermFTP-Client\\node_modules\\jest-circus\\runner.js",
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy",
      "^src/(.*)$": "<rootDir>/src/$1"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "resetMocks": true
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/fs-extra": "^9.0.7",
    "@types/ftp": "^0.3.31",
    "@types/history": "^4.7.8",
    "@types/react-redux": "^7.1.16",
    "@types/react-router": "^5.1.11",
    "@types/redux": "^3.6.0",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "electron": "^10.4.0",
    "electron-builder": "^22.9.1",
    "node-sass": "^4.14.1",
    "ts-loader": "^8.0.17",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.2.2",
    "wait-on": "^5.2.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
